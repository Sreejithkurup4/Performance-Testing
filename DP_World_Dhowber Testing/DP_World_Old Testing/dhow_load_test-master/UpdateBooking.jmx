            <stringProp name="script">import com.eclipsesource.json.JsonObject;
import com.eclipsesource.json.JsonArray;

String booking_details = vars.get(&quot;booking_ids&quot;);
JsonArray bookingDetails = JsonArray.readFrom(booking_details);
ArrayList bookingIds = new ArrayList();
for (JsonObject value : bookingDetails) {
   bookingIds.add(value.get(&quot;id&quot;));
}
String idListString = bookingIds.toString();
vars.put(&quot;bookingIdString&quot;, idListString);
vars.put(&quot;bookingLoopCount&quot;, bookingIds.size().toString());
