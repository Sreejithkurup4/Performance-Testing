            <stringProp name="script">import com.eclipsesource.json.JsonObject;
import com.eclipsesource.json.JsonArray;

String response = vars.get(&quot;trip_search_result&quot;);
JsonObject tripDetails = JsonObject.readFrom(response);
JsonArray trips = tripDetails.get(&quot;trips&quot;);
ArrayList tripIdList = new ArrayList();
ArrayList carrierIdList = new ArrayList();
for (JsonObject value : trips) {
	tripIdList.add(value.get(&quot;id&quot;));
	carrierIdList.add(value.get(&quot;vessel&quot;).get(&quot;account&quot;));
}
String tripIdListString = tripIdList.toString();
String carrierIdListString = carrierIdList.toString();
vars.put(&quot;tripIdListString&quot;, tripIdListString);
vars.put(&quot;carrierIdListString&quot;, carrierIdListString);
vars.put(&quot;tripLoopCount&quot;, tripIdList.size().toString());
int startIndex = 0;
vars.put(&quot;startIndex&quot;, startIndex.toString())





















</stringProp>
