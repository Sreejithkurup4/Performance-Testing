        <hashTree>
          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="[JSR223] Calculate time for first chunk" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import java.text.DateFormat;
import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Date;
import java.util.TimeZone;

DateFormat  dateFormat = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&apos;Z&apos;&quot;);
Date nowTime = dateFormat.parse(prev.getResponseDataAsString());
Date availabilityStartTime = dateFormat.parse(vars.get(&quot;startTime&quot;));

Date    now                 = Calendar.getInstance().getTime();
long    secondsDifference   = (nowTime.getTime() - availabilityStartTime.getTime()) / 1000;
long    segmentDuration     = 20000000;
long    segmentTimescalce   = 10000000;
double  segmentDurationSec  = (double)segmentDuration / (double) segmentTimescalce;
long    startNumber         = 1;
long    chunkNumber          = (long) (startNumber +  secondsDifference / segmentDurationSec) - 5;
log.info(vars.get(&quot;startTime&quot;));
log.info(Long.toString(chunkNumber));
vars.put(&quot;chunkId&quot;, Long.toString(chunkNumber));</stringProp>
          </JSR223PostProcessor>
